# Copy this file to .env and fill in your actual keys.
#   cp .env.example .env

# --- Blaxel AI (Model Gateway) ---
BLAXEL_API_KEY=bl_your_api_key_here
BLAXEL_WORKSPACE=rs
BLAXEL_MODEL_NAME=sandbox-openai

# Optional alias used by some scripts/docs
BL_API_KEY=bl_your_api_key_here

# Dashboard target (local by default; set to Blaxel URL for cloud demos)
VYUHA_API_URL=http://localhost:8000

# --- Google Gemini (legacy, optional) ---
GEMINI_API_KEY=your_gemini_api_key_here

# --- White Circle AI (Security Guard-Rail) ---
WHITE_CIRCLE_API_KEY=wc_your_api_key_here
WHITE_CIRCLE_DEPLOYMENT_ID=your_deployment_id_here
WHITE_CIRCLE_POLICY_ID=your_policy_id_here
WHITE_CIRCLE_BASE_URL=https://us.whitecircle.ai
WHITE_CIRCLE_VERSION=2025-12-01
